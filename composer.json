{
    "name": "nhzex/think-phinx",
    "type": "library",
    "description": "",
    "keywords": [
        "thinkphp",
        "thinkphp6",
        "blade"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "topthink/framework": "^6.0",
        "topthink/think-orm": "^2.0.27",
        "robmorgan/phinx": "~0.11.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8|^7",
        "symfony/var-dumper": "^5.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "HZEX\\Phinx\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Phinx\\": "tests"
        }
    },
    "extra": {
        "think": {
            "services": [
                "HZEX\\Phinx\\Service"
            ],
            "config": {
                "phinx": "config/phinx.php"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
