{
    "name": "nhzex/think-phinx",
    "type": "library",
    "description": "",
    "keywords": [
        "thinkphp",
        "thinkphp6",
        "phinx"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "robmorgan/phinx": "~0.12.5",
        "topthink/framework": "^6.0.6",
        "topthink/think-orm": "^2.0.30",
        "zxin/phinx-schema": "^1.0",
        "zxin/think-symfony-bridge": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8|^9",
        "symfony/var-dumper": "^5.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "HZEX\\Phinx\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Phinx\\": "tests"
        }
    },
    "extra": {
        "think": {
            "services": [
                "HZEX\\Phinx\\Service"
            ],
            "config": {
                "phinx": "config/phinx.php"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
